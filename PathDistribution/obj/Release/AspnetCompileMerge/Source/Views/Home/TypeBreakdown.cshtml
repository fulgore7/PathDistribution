@model IEnumerable<PathDistribution.Models.TypeBreakdown>

<table style="width:100%">
    <tr class="text2" style="text-align:center;padding-top:5px;">
        <th>@Html.DisplayNameFor(model => model.chrPath)</th>
        <th>@Html.DisplayNameFor(model => model.SK)</th>
        <th>@Html.DisplayNameFor(model => model.GI)</th>
        <th>@Html.DisplayNameFor(model => model.GU)</th>
        <th>@Html.DisplayNameFor(model => model.BRST)</th>
        <th>@Html.DisplayNameFor(model => model.FNA)</th>
        <th>@Html.DisplayNameFor(model => model.NCB)</th>
        <th>@Html.DisplayNameFor(model => model.GP)</th>
        <th>@Html.DisplayNameFor(model => model.PBX)</th>
        <th>@Html.DisplayNameFor(model => model.BX)</th>
        <th>@Html.DisplayNameFor(model => model.BB)</th>
        <th>@Html.DisplayNameFor(model => model.WS)</th>
        <th>@Html.DisplayNameFor(model => model.CYTO)</th>
        <th>@Html.DisplayNameFor(model => model.TOTAL)</th>
    </tr>

@foreach (var item in Model)
{
    <tr class="text5" style="text-align:left;">
        <td style="text-align:center">@Html.DisplayFor(modelItem => item.chrPath)</td>
        <td style="text-align:center">@Html.DisplayFor(modelItem => item.SK)</td>
        <td style="text-align:center">@Html.DisplayFor(modelItem => item.GI)</td>
        <td style="text-align:center">@Html.DisplayFor(modelItem => item.GU)</td>
        <td style="text-align:center">@Html.DisplayFor(modelItem => item.BRST)</td>
        <td style="text-align:center">@Html.DisplayFor(modelItem => item.FNA)</td>
        <td style="text-align:center">@Html.DisplayFor(modelItem => item.NCB)</td>
        <td style="text-align:center">@Html.DisplayFor(modelItem => item.GP)</td>
        <td style="text-align:center">@Html.DisplayFor(modelItem => item.PBX)</td>
        <td style="text-align:center">@Html.DisplayFor(modelItem => item.BX)</td>
        <td style="text-align:center">@Html.DisplayFor(modelItem => item.BB)</td>
        <td style="text-align:center">@Html.DisplayFor(modelItem => item.WS)</td>
        <td style="text-align:center">@Html.DisplayFor(modelItem => item.CYTO)</td>
        <td style="text-align:center">@Html.DisplayFor(modelItem => item.TOTAL)</td>
    </tr>
}
    <tr>
        <td colspan="13" style="padding-top:10px;">
            <button id="closeBreakdown" class="cartbutton" type="button" style="float:right;margin-right:15px;margin-bottom:5px;">Close</button>
        </td>
    </tr>
</table>
